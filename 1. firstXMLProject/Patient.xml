    <tns:name>Beppe</tns:name>
    <tns:surname>Franco</tns:surname>
    <tns:age>123</tns:age>
    <tns:gender>M</tns:gender>
    <tns:phone>+39 3516698350</tns:phone>
    <tns:paymentType>
        <!--        <tns:cash>10.5</tns:cash>-->
        <!--  ! Scelta se cash o  assicurazione -->
                <tns:insurance>
                                <tns:provider>Red Prima Solution</tns:provider>
                                <tns:limit>123</tns:limit>
                </tns:insurance>
    </tns:paymentType>

</tns:Patient>